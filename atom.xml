layout: nil
---

<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

 <title>zacharylew.is</title>
 <link href="http://zacharylew.is/atom.xml" rel="self"/>
 <link href="http://zacharylew.is/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://zacharylew.is/</id>
 <author>
   <name>Zachary Lewis</name>
   <email>contact@zacharylew.is</email>
 </author>

 {% for post in site.posts %}
 <entry>
   <title>{{ post.title }}</title>
   <link href="http://zacharylew.is{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>http://zacharylew.is{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endfor %}

</feed>
